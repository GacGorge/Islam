@phdthesis{rinder_modelisation_2016,
	title = {Modélisation des sinistres corporels en automobile},
	url = {https://www.institutdesactuaires.com/se-documenter/memoires/memoires-d-actuariat-4651?id=122aa2674867aeb26529af27d12995c1},
	abstract = {Les sinistres corporels en automobile sont des évènements très couteux pour les compagnies d’assurance. Cependant, de nombreuses sociétés mutualisent entièrement ce risque ou le tarifent de manière très simple. En effet, le caractère rare de ce type de sinistre rend l’exercice de prédiction plus complexe. Ce mémoire expose les principaux résultats faisant suite à l’exploration de nouvelles pistes permettant d’affiner la connaissance du risque de sinistre corporel. Dans un premier temps, la fréquence de ces sinistres sur le marché turc est étudiée. Les données récoltées par AXA Sigorta (filiale turque d’AXA) comprennent entre autre des informations sur la sinistralité passée des assurés, sous la forme du nombre de sinistres durant les quatre dernières années. Ces informations font l’objet d’une analyse particulière qui permet d’identifier des critères de tarification et de gestion de portefeuille. Par la suite, les résultats d’un modèle linéaire généralisé sont confrontés avec ceux d’un modèle issu de Vowpal Wabbit, programme utilisant un autre algorithme pour identifier les profils à risque. Dans un second temps, la sévérité des accidents corporels est modélisée en exploitant la base de données publique des sinistres corporels en 2012 sur le territoire français, fournie par l’ONISR (Observatoire National Interministériel de la Sécurité Routière). L’objectif est de comprendre l’influence du contexte de conduite sur la sévérité de l’accident. Ainsi l’influence de la conduite en campagne, par temps de pluie, de nuit ou sur autoroute est quantifiée afin de pouvoir donner à chaque contexte de conduite un score reflétant le risque de blessure grave ou de décès en cas d’accident.},
	author = {Rinder, Pierre},
	year = {2016},
}

@phdthesis{chargueraud_alice_taux_2016,
	title = {Le taux de transformation en automobile : comparaison de différentes méthodes d{\textbackslash}'apprentissage},
	url = {https://www.institutdesactuaires.com/se-documenter/memoires/memoires-d-actuariat-4651?id=d98a0b4b61c2cf7e454959da80c81e4c},
	abstract = {Dans un univers en pleine évolution face à l'abondance des données, plus généralement dénommé "Big Data", les actuaires doivent nécessairement adapter leurs outils pour progresser dans l'analyse et la prédiction des comportements. Notre étude propose une analyse comparative de différentes méthodes d'apprentissage, ainsi que de leurs résultats en matière de prédiction. En effet, les méthodes d'agrégation appliquées aux arbres de régression semblent proposer des solutions performantes et robustes, et qui présentent, de plus, l'avantage d'être appropriées dans le cadre d'un environnement formé d'un nombre important de variables explicatives. L'application à un cas concret - la modélisation du taux de transformation en assurance automobile - a permis de mieux comprendre le fonctionnement de chacune de ces méthodes statistiques. L'apprentissage automatique repose sur des principes fondamentaux assez différents de la théorie du modèle linéaire généralisé. Bien que toutes les méthodes issues de l'apprentissage automatique aient un socle commun, nous avons pu mettre en évidence leurs particularités. Une comparaison des différentes implémentations a permis de conclure, que pour notre jeu de données, les prédictions apportées par l'algorithme des arbres de régression boostés étaient les plus performantes. L'effet "boîte noire" des algorithmes des arbres de régression et des forêts aléatoires, ainsi que la perte de lisibilité des résultats qui en découle, justifie que la régression logistique, et plus généralement le modèle linéaire généralisé, reste aujourd'hui un outil très populaire dans le secteur de la tarification non-vie.},
	author = {CHARGUERAUD Alice},
	year = {2016},
}

